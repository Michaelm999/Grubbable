<h1>Log In</h1>
<%= form_for :login, {url: login_path} do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit 'Log In' %>
  </div>
<% end %>
<div class="back">
<%= link_to('Go Back', root_path) %>
</div>
